! urxvt

URxvt*geometry:     80x30

URxvt*font:			xft:Ubuntu Mono:pixelsize=20
URxvt*boldfont:		xft:FreeMono:pixelsize=17
URxvt*depth:		24
URxvt*borderless:	1
URxvt*scrollBar:    false
URxvt*saveLines:	2000

Xft.dpi:			166
Xft.antialias:		true
Xft.hinting:		true

URxvt*shading:		16

!urxvt color scheme:
URxvt*background: #262626
URxvt*foreground: #DEDEDE

! black
*color0	:	#073642
*color8	:	#002B36
! red
*color1	:	#DC322F
*color9	:	#CB4B16
! green
*color2	:	#859900
*color10	:	#586E75
! yellow
*color3	:	#B58900
*color11	:	#657B83
